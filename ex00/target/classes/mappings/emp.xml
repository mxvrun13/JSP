<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.app.impl.EmpMapper">
	<select id="getEmp" resultType="emp" parameterType="String">
		select employee_id, first_name, job_id from employees where department_id = #{value}
	</select>
	<select id="getDept" resultType="departments">
		select * from departments
	</select>
</mapper>